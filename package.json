{
  "name": "lab_2_it",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "packager": "react-native start --reset-cache",
    "pods": "cd ios && pod install",
    "android": "react-native run-android",
    "android-release": "react-native run-android --mode=release",
    "ios": "npx react-native run-ios --mode Debug",
    "ios-15-pro": "react-native run-ios --simulator='iPhone 15 Pro'",
    "ios-release": "npx react-native run-ios --mode Release",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint src --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start": "react-native start",
    "test": "jest --passWithNoTests",
    "prepare": "husky install",
    "add-pre-commit": "npx husky add .husky/prepare-commit-msg 'npx jira-prepare-commit-msg $1'",
    "format": "prettier ./src --write",
    "clean-android": "cd android && ./gradlew clean"
  },
  "dependencies": {
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@react-native-clipboard/clipboard": "^1.12.1",
    "@react-native-masked-view/masked-view": "^0.3.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "axios": "^1.6.2",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-image-picker": "^7.1.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "^3.6.1",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^14.0.0",
    "react-native-svg-transformer": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.10",
    "@types/react": "^18.2.40",
    "@types/react-native": "^0.72.7",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.55.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
